{
  "name": "@tadashi/find-in-cache",
  "version": "1.1.0",
  "description": "Find data in cache",
  "main": "src/index.js",
  "files": [
    "src",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "pretest": "xo",
    "test": "c8 --reporter=text --reporter=lcov ava"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/lagden/find-in-cache.git"
  },
  "keywords": [
    "cache",
    "redis",
    "lib",
    "find"
  ],
  "engines": {
    "node": ">=12"
  },
  "author": "Thiago Lagden",
  "license": "MIT",
  "homepage": "https://github.com/lagden/find-in-cache#readme",
  "bugs": {
    "url": "https://github.com/lagden/find-in-cache/issues"
  },
  "xo": {
    "envs": [
      "node"
    ],
    "semicolon": false,
    "rules": {
      "camelcase": [
        "error",
        {
          "properties": "never"
        }
      ],
      "capitalized-comments": 0,
      "unicorn/prevent-abbreviations": 0
    }
  },
  "dependencies": {
    "@tadashi/cache-redis": "5.0.1",
    "@tadashi/hash": "1.0.0"
  },
  "peerDependencies": {
    "ioredis": ">= 4.19.4 < 5"
  },
  "devDependencies": {
    "ava": "3.15.0",
    "c8": "7.7.1",
    "ioredis": "4.27.1",
    "xo": "0.39.1"
  }
}
